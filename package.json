{
  "name": "ldbc-snb-enhancer",
  "version": "2.5.2",
  "packageManager": "yarn@1.22.22",
  "description": "Generates auxiliary data based on an LDBC SNB dataset",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/SolidBench/ldbc-snb-enhancer.js#readme",
  "repository": "git@github.com:SolidBench/ldbc-snb-enhancer.js.git",
  "lsd:module": true,
  "bugs": {
    "url": "https://github.com/SolidBench/ldbc-snb-enhancer.js/issues"
  },
  "keywords": [
    "rdf",
    "rdfjs",
    "dataset",
    "ldbc",
    "snb",
    "social network"
  ],
  "main": "lib/index.js",
  "typings": "lib/index",
  "bin": {
    "ldbc-snb-enhancer": "bin/runner.js"
  },
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "bin/**/*.js.map",
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "lib/**/*.json"
  ],
  "engines": {
    "node": ">=12.0"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "lint": "eslint . --cache",
    "build": "yarn run build:ts && yarn run build:components",
    "build:components": "componentsjs-generator -s lib",
    "build:ts": "tsc",
    "validate": "yarn list",
    "prepare": "husky && yarn run build",
    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "@rdfjs/types": "*",
    "componentsjs": "^6.0.0",
    "rdf-object": "^3.0.0",
    "rdf-parse": "^3.0.0",
    "rdf-serialize": "^3.0.0",
    "rdf-string": "^1.6.0",
    "rdf-terms": "^2.0.0",
    "relative-to-absolute-iri": "^1.0.0"
  },
  "devDependencies": {
    "@rubensworks/eslint-config": "^3.0.0",
    "@types/jest": "^29.0.0",
    "arrayify-stream": "^1.0.0",
    "componentsjs-generator": "^4.0.0",
    "eslint": "^8.0.0",
    "husky": "^9.0.0",
    "jest": "^28.0.0",
    "jest-rdf": "^1.0.0",
    "manual-git-changelog": "^1.0.0",
    "rdf-data-factory": "^1.1.0",
    "stream-to-string": "^1.2.0",
    "streamify-array": "^1.0.1",
    "streamify-string": "^1.0.1",
    "ts-jest": "^28.0.0",
    "typescript": "^5.0.0"
  }
}
